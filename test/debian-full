#!/usr/bin/env sh
# Usage: \. /dev/stdin <<< "$(wget -qO- https://raw.githubusercontent.com/lixpose/scripts/master/test/debian-full)"

LIX_TEST_BRANCH="${LIX_TEST_BRANCH:-master}"

trap 'echo "$ $BASH_COMMAND"' DEBUG

\. /dev/stdin <<< "$(wget -qO- https://raw.githubusercontent.com/lixpose/scripts/$LIX_TEST_BRANCH/init)"
lixreq prep/debian
lixreq mbx-toolbox
lixreq python3
lixreq node 18
lixreq lemp 8.1
lixreq composer

trap - DEBUG

