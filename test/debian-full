#!/usr/bin/env bash
# Usage: \. /dev/stdin <<< "$(wget -qO- https://raw.githubusercontent.com/lxc-compose/scripts/master/test/debian-full)"

LXC_TEST_BRANCH="${LXC_TEST_BRANCH:-master}"

trap 'echo "$ $BASH_COMMAND"' DEBUG

\. /dev/stdin <<< "$(wget -qO- https://raw.githubusercontent.com/lxc-compose/scripts/$LXC_TEST_BRANCH/init)"
lxc-setup prep/debian
lxc-setup mbx-toolbox
lxc-setup python3
lxc-setup node 18
lxc-setup lemp 8.1
lxc-setup composer

trap - DEBUG

